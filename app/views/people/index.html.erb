<h1>Registered Dispatchees</h1>

<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Phone #</th>
    <th>Email</th>
  </tr>

<% @people.each do |person| %>
  <tr>
    <td><%= person.first_name %></td>
    <td><%= person.last_name %></td>
    <td><%= person.primary_phone_number %></td>
    <td><%= person.email %></td>
  </tr>
<% end %>
</table>
